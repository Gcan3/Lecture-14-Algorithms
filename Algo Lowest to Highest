#include <iostream>
#include <string>
#include <array>
#include <algorithm>

using namespace std;
int main()
{
    array<int, 5> numbers = { 33,5,7,99,83 };
    sort(numbers.begin(), numbers.end()); //call to sort
    reverse(numbers.begin(), numbers.end()); //reverse the output
    for (int num : numbers) {
        cout << num << " "; //output should be 99,83,33,7,5
    }
}
